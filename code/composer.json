{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0.2",
    "arrilot/laravel-widgets": "^3.13.2",
    "bacon/bacon-qr-code": "^2.0.8",
    "barryvdh/laravel-dompdf": "^v2.0.1",
    "barryvdh/laravel-ide-helper": "^v2.13.0",
    "binarytorch/larecipe": "^v2.6.1",
    "consoletvs/charts": "^6.6.0",
    "google/recaptcha": "^1.3.0",
    "guzzlehttp/guzzle": "^7.8.0",
    "laracasts/flash": "^3.2.2",
    "laracasts/utilities": "^3.2.2",
    "laravel/framework": "^v10.31.0",
    "laravel/passport": "^v11.10.0",
    "laravel/sanctum": "^v3.3.2",
    "laravel/tinker": "^v2.8.2",
    "laravel/ui": "^v4.2.2",
    "livewire/livewire": "^v3.1.0",
    "maatwebsite/excel": "^3.1.50",
    "milon/barcode": "^v10.0.1",
    "nesbot/carbon": "^2.71.0",
    "nwidart/laravel-modules": "^10.0.3",
    "paynow/php-sdk": "^1.0.5",
    "paypal/paypal-checkout-sdk": "^1.0.2",
    "pragmarx/google2fa-laravel": "^v2.1.1",
    "spatie/laravel-activitylog": "^4.6.0",
    "spatie/laravel-backup": "^8.4.0",
    "spatie/laravel-permission": "^6.0.1",
    "stripe/stripe-php": "^v13.2.1",
    "yajra/laravel-datatables-oracle": "^v10.11.0"
  },
  "require-dev": {
    "fakerphp/faker": "^v1.23.0",
    "barryvdh/laravel-debugbar": "^v3.9.2",
    "laravel/telescope": "^v4.17.2",
    "mockery/mockery": "^1.6.5",
    "nunomaduro/collision": "^v7.10.0",
    "phpunit/phpunit": "^9.5.10",
    "phpstan/phpstan": "^1.10.41",
    "spatie/laravel-ignition": "^2.3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    },
    "files": [
      "Modules/Core/Helpers/general_helper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
